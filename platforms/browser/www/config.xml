<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aa.tipline" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Tip Line</name>
    <description>
        Tip Line Application for Airline Ambassadors.
    </description>
    <author email="aa@gmail.com" href="http://airlineamb.org">
        GA Tech Tip Line Team: Kenta Kawaguchi, Micah Jo, Heather Schirra, Kyle Al-Shafei, William Cheng
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" platform="android" src="www/lib/assets/ic_launcher-web.png" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
</widget>
